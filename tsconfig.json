{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"moduleResolution": "Node",
		"importHelpers": true,
		"removeComments": true,
		"esModuleInterop": true,
		"sourceMap": false,
		"declaration": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"pretty": true,
		"diagnostics": true,
		"allowJs": true,
		"checkJs": true,
		"alwaysStrict": true,
		"incremental": true,
		"outDir": "./dist"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
